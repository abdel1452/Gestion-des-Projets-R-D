{% extends 'project/base.html.twig' %}

{% block title %}Ajouter un projet{% endblock %}

{% block body %}
<div class="container mt-4">
    <h1>Ajouter un projet</h1>
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.prodUrl) }}
    {{ form_row(form.preprodUrl) }}
    {{ form_row(form.imageFile) }}
    <button class="btn btn-primary mt-3">Ajouter</button>
{{ form_end(form) }}


    <a href="{{ path('project_index') }}" class="btn btn-link mt-3">Retour Ã  la liste des projets</a>
</div>
{% endblock %}
